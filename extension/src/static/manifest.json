{
	"name": "Bolt",
	"version": "0.0.0.01",
	"description": "Apply to more jobs in less time.",
	"manifest_version": 3,
	"permissions": ["activeTab", "scripting", "tabs"],
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["content.js"],
			"run_at": "document_end"
		}
	],
	"action": {
		"default_icon": {
			"16": "images/off-icon-16.png",
			"32": "images/off-icon-32.png",
			"48": "images/off-icon-48.png",
			"128": "images/off-icon-128.png"
		}
	},
	"icons": {
		"16": "images/on-icon-16.png",
		"32": "images/on-icon-32.png",
		"48": "images/on-icon-48.png",
		"128": "images/on-icon-128.png"
	},
	"web_accessible_resources": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"resources": ["images/*", "frame.html", "script.js", "bime.js"]
		}
	]
}
